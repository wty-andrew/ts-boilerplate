{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "dev:client": "cross-env NODE_ENV=development parcel serve src/client/index.html -p 3001 --open",
    "dev:server": "cross-env NODE_ENV=development nodemon src/server/index.ts",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "build:client": "cross-env NODE_ENV=production parcel build src/client/index.html --dist-dir dist/client",
    "build:server": "cross-env NODE_ENV=production tsc --build",
    "build": "yarn build:server && yarn build:client",
    "test:client": "cross-env NODE_ENV=test jest --selectProjects client",
    "test:server": "cross-env NODE_ENV=test jest --runInBand --selectProjects server",
    "test": "concurrently \"yarn test:client\" \"yarn test:server\"",
    "lint": "eslint src",
    "prebuild": "rimraf dist"
  },
  "devDependencies": {
    "@parcel/transformer-glsl": "2.7.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@types/three": "^0.144.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "buffer": "^6.0.3",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-extended": "^3.1.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.7.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@react-spring/three": "^9.5.5",
    "@react-three/cannon": "^6.4.0",
    "@react-three/drei": "^9.36.0",
    "@react-three/fiber": "^8.8.10",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "history": "^5.3.0",
    "leva": "^0.9.32",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "regenerator-runtime": "^0.13.10",
    "sanitize.css": "^13.0.0",
    "three": "^0.145.0",
    "zustand": "^4.1.3"
  },
  "staticFiles": {
    "staticPath": "public"
  }
}
