{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "dev:client": "cross-env NODE_ENV=development parcel serve src/client/index.html -p 3001 --open",
    "dev:server": "cross-env NODE_ENV=development nodemon src/server/index.ts",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "build:client": "cross-env NODE_ENV=production parcel build src/client/index.html --dist-dir dist/client",
    "build:server": "cross-env NODE_ENV=production tsc --build",
    "build": "yarn build:server && yarn build:client",
    "test:client": "cross-env NODE_ENV=test jest --selectProjects client",
    "test:server": "cross-env NODE_ENV=test jest --runInBand --selectProjects server",
    "test": "concurrently \"yarn test:client\" \"yarn test:server\"",
    "lint": "eslint src",
    "prebuild": "rimraf dist"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.3",
    "@types/nodemailer": "^6.4.6",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.9",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-extended": "^3.1.0",
    "mongodb-memory-server": "^8.9.3",
    "nodemon": "^2.0.20",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "history": "^5.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.6.7",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "regenerator-runtime": "^0.13.10",
    "sanitize.css": "^13.0.0"
  }
}
